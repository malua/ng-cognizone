{
  "name": "model-utils",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/utils/model-utils/src",
  "projectType": "library",
  "tags": ["type:util", "scope:shared"],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "main": "libs/shared/utils/model-utils/src/index.ts",
        "outputPath": "dist/libs/shared/utils/model-utils",
        "tsConfig": "libs/shared/utils/model-utils/tsconfig.lib.json",
        "compiler": "babel",
        "project": "libs/shared/utils/model-utils/package.json",
        "format": ["esm", "cjs"],
        "generateExportsField": true,
        "additionalEntryPoints": [],
        "assets": [
          {
            "glob": "libs/shared/utils/model-utils/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/shared/utils/model-utils"],
      "options": {
        "jestConfig": "libs/shared/utils/model-utils/jest.config.ts"
      }
    }
  }
}
