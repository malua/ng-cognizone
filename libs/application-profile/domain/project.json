{
  "name": "application-profile",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/application-profile/domain/src",
  "projectType": "library",
  "tags": ["type:domain", "scope:application-profile"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/application-profile/domain",
        "main": "libs/application-profile/domain/src/index.ts",
        "tsConfig": "libs/application-profile/domain/tsconfig.lib.json",
        "assets": ["libs/application-profile/domain/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/application-profile/domain"],
      "options": {
        "jestConfig": "libs/application-profile/domain/jest.config.ts"
      }
    }
  }
}
